.PHONY: clean All

All:
	@echo "----------Building project:[ flipflop_irq - Debug ]----------"
	@cd "flipflop_irq" && "$(MAKE)" -f  "flipflop_irq.mk" && "$(MAKE)" -f  "flipflop_irq.mk" PostBuild
clean:
	@echo "----------Cleaning project:[ flipflop_irq - Debug ]----------"
	@cd "flipflop_irq" && "$(MAKE)" -f  "flipflop_irq.mk" clean
